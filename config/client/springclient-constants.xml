<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="COMP_LEFT" class="java.lang.Float"><constructor-arg value="0.0" /></bean>
	<bean id="COMP_CENTER" class="java.lang.Float"><constructor-arg value="0.5" /></bean>
	<bean id="COMP_RIGHT" class="java.lang.Float"><constructor-arg value="1.0" /></bean>
	
	<bean id="GRIDPANEL_LEFT" class="java.lang.Integer"><constructor-arg value="17" /></bean>
	<bean id="GRIDPANEL_CENTER" class="java.lang.Integer"><constructor-arg value="10" /></bean>
	<bean id="GRIDPANEL_RIGHT" class="java.lang.Integer"><constructor-arg value="13" /></bean>
</beans>
